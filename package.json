{
  "name": "ui-components",
  "version": "1.0.0",
  "private": true,
  "description": "A collection of react components used by several internal projects.",
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel --copy-files --out-dir lib src",
    "watch": "babel --watch --copy-files --out-dir lib src"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "dependencies": {
    "react-addons-create-fragment": "^15.3.0",
    "lodash": "^3.10.1"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.0",
    "eslint": "^3.3.1",
    "eslint-plugin-flowtype": "^2.11.4",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2"
  },
  "peerDependencies": {
    "i18next": ">=2.0.4",
    "react": "^15.3.0",
    "moment": "^2.10.6",
    "moment-timezone": "^0.4.1",
    "react-bootstrap": "^0.30.0",
    "react-datagrid": "^2.1.1",
    "react-router": ">=2.0.0",
    "react-dom": "^15.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vpon/ui-components.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Frontend",
  "license": "Apache License Version 2.0",
  "bugs": {
    "url": "https://github.com/vpon/ui-components/issues"
  },
  "homepage": "https://github.com/vpon/ui-components#readme"
}
